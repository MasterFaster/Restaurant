<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="296.0" prefWidth="305.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.peopleOperations.GetRegularClientInfoController">
    <children>
        <HBox alignment="CENTER" prefHeight="36.0" prefWidth="305.0">
            <children>
                <Label alignment="CENTER" contentDisplay="TOP" text="First Name" />
                <TextField fx:id="firstNameField" alignment="CENTER" prefWidth="149.0" promptText="">
                    <opaqueInsets>
                        <Insets />
                    </opaqueInsets>
                    <HBox.margin>
                        <Insets left="10.0" />
                    </HBox.margin>
                </TextField>
            </children>
        </HBox>
        <HBox alignment="CENTER" prefHeight="37.0" prefWidth="305.0">
            <children>
                <Label text="Second Name" />
                <TextField fx:id="secondNameField" alignment="CENTER">
                    <HBox.margin>
                        <Insets left="10.0" />
                    </HBox.margin>
                </TextField>
            </children>
            <VBox.margin>
                <Insets />
            </VBox.margin>
        </HBox>
        <HBox alignment="CENTER">
            <children>
                <Label text="Location" />
            <ListView fx:id="possibleLocationsListView" prefHeight="65.0" prefWidth="200.0" />
            </children>
        </HBox>
        <HBox alignment="CENTER" prefHeight="35.0" prefWidth="305.0">
            <children>
                <Label text="Code" />
                <TextField fx:id="codeField">
                    <HBox.margin>
                        <Insets left="10.0" />
                    </HBox.margin>
                </TextField>
            </children>
        </HBox>
        <HBox alignment="CENTER" prefHeight="36.0" prefWidth="305.0">
            <children>
                <Label text="Phone Number" />
                <TextField fx:id="phoneNrField">
                    <opaqueInsets>
                        <Insets />
                    </opaqueInsets>
                    <HBox.margin>
                        <Insets left="10.0" />
                    </HBox.margin>
                </TextField>
            </children>
        </HBox>
        <HBox alignment="CENTER" prefHeight="35.0" prefWidth="305.0">
            <children>
                <Label text="Email" />
                <TextField fx:id="emailField">
                    <HBox.margin>
                        <Insets left="10.0" />
                    </HBox.margin>
                </TextField>
            </children>
        </HBox>
      <HBox alignment="CENTER" prefHeight="39.0" prefWidth="305.0">
         <children>
            <Label text="Awards Credits" />
            <TextField fx:id="awardsCreditsField">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </TextField>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="34.0" prefWidth="305.0">
         <children>
            <Label text="Regular Discount" />
            <TextField fx:id="regularDiscountField">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </TextField>
            <Label text="\%" />
         </children>
      </HBox>
        <HBox alignment="CENTER" prefHeight="40.0" prefWidth="305.0">
            <children>
                <Button fx:id="addClientBtn" mnemonicParsing="false" onAction="#addClient" text="Add">
                    <opaqueInsets>
                        <Insets />
                    </opaqueInsets>
                    <HBox.margin>
                        <Insets right="30.0" />
                    </HBox.margin>
                </Button>
                <Button fx:id="cancelAddClientBtn" mnemonicParsing="false" onAction="#cancelAddingClient" text="Cancel" />
            </children>
        </HBox>
    </children>
</VBox>
